{
  "name": "compliance-management-backend",
  "version": "1.0.0",
  "description": "Backend for compliance management system",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node dev-start.js",
    "dev:simple": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "dev:verbose": "nodemon --verbose src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node src/utils/migrate.js",
    "migrate:watch": "nodemon --exec 'node src/utils/migrate.js' --watch src/utils/migrate.js",
    "build": "echo 'No build step required for Node.js'",
    "postinstall": "node src/utils/migrate.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "isolation-forest": "^0.0.9",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "openai": "^4.104.0",
    "pg": "^8.11.3",
    "twilio": "^5.10.2"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "jest": "^29.6.2",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
